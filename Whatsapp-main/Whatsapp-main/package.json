{
  "name": "baileys-executable",
  "version": "1.0.0",
  "description": "Baileys WhatsApp Library - Executable Distribution",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && pkg . --output dist/baileys",
    "build-all": "tsc && pkg . --targets node18-win-x64,node18-linux-x64,node18-macos-x64 --output dist/baileys",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts"
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ],
    "assets": [
      "node_modules/@whiskeysockets/baileys/WAProto/**/*"
    ],
    "targets": [
      "node18-win-x64",
      "node18-linux-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^6.7.18",
    "baileys": "^6.7.18",
    "bcryptjs": "^3.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "nexe": "^5.0.0-beta.4",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "MIT",
  "keywords": [],
  "type": "commonjs"
}
