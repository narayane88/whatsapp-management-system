{
  "id": "baileys-whatsapp-api-env",
  "name": "Baileys WhatsApp API - Local Development",
  "values": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3005",
      "description": "Base URL for Baileys WhatsApp Server",
      "type": "default",
      "enabled": true
    },
    {
      "key": "frontendUrl",
      "value": "http://localhost:3000",
      "description": "Frontend Next.js application URL",
      "type": "default",
      "enabled": true
    },
    {
      "key": "testAccountId",
      "value": "",
      "description": "Test account ID - auto-generated during tests",
      "type": "default",
      "enabled": true
    },
    {
      "key": "testPhoneNumber",
      "value": "+1234567890",
      "description": "Test phone number for pairing code tests",
      "type": "default",
      "enabled": true
    },
    {
      "key": "testRecipientJID",
      "value": "1234567890@s.whatsapp.net",
      "description": "Test recipient JID for sending messages",
      "type": "default",
      "enabled": true
    },
    {
      "key": "testGroupJID",
      "value": "1234567890-1234567890@g.us",
      "description": "Test group JID for group messages",
      "type": "default",
      "enabled": true
    },
    {
      "key": "webhookUrl",
      "value": "https://webhook.site/your-unique-url-here",
      "description": "Webhook URL for receiving events (get from webhook.site)",
      "type": "default",
      "enabled": true
    },
    {
      "key": "connectedAccountId",
      "value": "",
      "description": "ID of a connected account for message testing",
      "type": "default",
      "enabled": true
    },
    {
      "key": "workflowAccountId",
      "value": "",
      "description": "Account ID used in workflow tests",
      "type": "default",
      "enabled": true
    }
  ],
  "_postman_variable_scope": "environment"
}