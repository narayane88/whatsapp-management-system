{
  "name": "whatsapp-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "WhatsApp Multi-Tier Management System with comprehensive v1 API",
  "keywords": [
    "whatsapp",
    "business",
    "management",
    "multi-tier",
    "nextjs",
    "api",
    "queue-based-messaging",
    "bulk-messaging"
  ],
  "author": "Development Team",
  "license": "MIT",
  "scripts": {
    "dev": "npm run kill-port && set PORT=3100 && next dev -p 3100 --turbo",
    "dev:clean": "npm run clean && npm run kill-port && next dev --turbo",
    "build": "next build",
    "start": "next start -p 3100",
    "start:prod": "NODE_ENV=production next start -p 3100",
    "start:clean": "npm run build && npm run start",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop whatsapp-frontend",
    "pm2:restart": "pm2 restart whatsapp-frontend",
    "pm2:delete": "pm2 delete whatsapp-frontend",
    "pm2:logs": "pm2 logs whatsapp-frontend",
    "pm2:monit": "pm2 monit",
    "deploy": "./deploy-prod.sh deploy",
    "kill-port": "npx kill-port 3100",
    "kill-port:prod": "npx kill-port 3100",
    "kill-all": "npx kill-port 3100 5432 6379",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed-simple.ts",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "test:whatsapp": "npx tsx src/scripts/test-whatsapp-api.ts",
    "test:whatsapp:quick": "npx ts-node -e \"import('./src/__tests__/utils/whatsapp-test-runner').then(m => m.quickTest())\"",
    "validate:whatsapp": "npx tsx src/scripts/validate-whatsapp-integration.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --verbose",
    "test:unit": "jest --testPathPattern=src",
    "test:api": "node tests/api-integration.test.js",
    "test:api:comprehensive": "node ../comprehensive-api-test.js",
    "test:api:v1": "node ../comprehensive-api-test.js",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:customer": "jest --config=jest.config.customer.js",
    "test:customer:watch": "jest --config=jest.config.customer.js --watch",
    "test:customer:coverage": "jest --config=jest.config.customer.js --coverage",
    "test:all": "npm run test:unit && npm run test:customer && npm run test:api && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:customer:coverage && npm run test:api",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rimraf .next out dist",
    "docs:generate": "node scripts/generate-api-docs.js",
    "docs:serve": "npm run docs:generate && npx serve docs",
    "docs:watch": "nodemon --watch src/app/api --ext ts --exec \"npm run docs:generate\"",
    "hooks:setup": "node scripts/setup-hooks.js",
    "postinstall": "prisma generate && npm run hooks:setup",
    "fresh": "npm run clean && npm run kill-port && npm install && npm run db:generate && npm run dev"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@mantine/carousel": "^7.12.2",
    "@mantine/core": "^7.12.2",
    "@mantine/dates": "^7.12.2",
    "@mantine/form": "^7.12.2",
    "@mantine/hooks": "^7.12.2",
    "@mantine/notifications": "^7.12.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.13.0",
    "@tabler/icons-react": "^3.34.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.5",
    "@types/qrcode": "^1.5.5",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "embla-carousel-react": "^8.6.0",
    "jsonwebtoken": "^9.0.2",
    "lightningcss": "^1.30.1",
    "next": "^15.4.6",
    "next-auth": "^4.24.11",
    "next-swagger-doc": "^0.4.1",
    "nodemailer": "^6.10.1",
    "pg": "^8.16.3",
    "prisma": "^6.13.0",
    "qrcode": "^1.5.4",
    "razorpay": "^2.9.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swr": "^2.3.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.6",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vercel/ncc": "^0.38.3",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "kill-port": "^2.0.1",
    "node-fetch": "^3.3.2",
    "pkg": "^5.8.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ]
}
